<script>
var userAv = '<%- user[0].avatar -%>'
function getAv() {
    $('.avatarimg').attr("src","/_assets_/uploads/" + userAv)
}
function setTheme() {
    var themecss = '<%- user[0].currentTheme -%>'
    $('.currentTheme').attr("href",'/_assets_/css/' + themecss + '.css')
}
function checkAd() {
    var userLevel = '<%- user[0].adminLevel -%>'
    var url = window.location.pathname
    if (userLevel = 'admin' && url.split('/')[1] == 'Network') {
        $('.adcheck').attr("onclick", "window.location.href = '/Admin.Network/Home'")
        $('.adchecktext').text("Admin Panel")
        $('.adcheck').removeClass("hidden")
    }
}
getAv();
setTheme();
checkAd();
</script>